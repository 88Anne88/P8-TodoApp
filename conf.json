{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",   
        "include": ["./js","./node_modules/jsdoc/lib","package.json","README.md"], 
        "exclude": ["node_modules"]           
    },
    "recurseDepth": 10,                       
    "opts": {
        "destination": "./docs/",              
        "recurse": true,                         
        "encoding": "utf8",
        "private": true,
        "template": "./node_modules/jsdoc/templates/jsdoc-template"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "plugins": [
        "plugins/markdown"

    ],
    "templates": {
        "referenceTitle": "My todoApp",
        "disableSort": false,
        "collapse": true   
    }

}




